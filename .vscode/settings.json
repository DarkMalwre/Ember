{
    "files.exclude": {
        "!**/shims-vue.d.ts": true,
        "!shims-vue.d.ts": true,
        "*.js": true,
        "**/*.d.ts": true,
        "**/*.js": true,
        "**/*.map": true
    },
    "search.exclude": {
        "*.js": true
    },
    "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
    "vetur.experimental.templateInterpolationService": true,
    "emmet.includeLanguages": {
        "vue": "html",
        "vue-html": "html"
    }
}